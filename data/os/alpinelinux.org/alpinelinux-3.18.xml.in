  <os id="http://alpinelinux.org/alpinelinux/3.18">
    <short-id>alpinelinux3.18</short-id>
    <name>Alpine Linux 3.18</name>
    <version>3.18</version>
    <vendor>Alpine Linux Project</vendor>
    <family>linux</family>
    <distro>alpinelinux</distro>
    <upgrades id="http://alpinelinux.org/alpinelinux/3.17"/>
    <derives-from id="http://alpinelinux.org/alpinelinux/3.17"/>

    <release-date>2023-05-09</release-date>
    <eol-date>2025-05-09</eol-date>

    <resources arch="all">
      <minimum>
        <cpu>1000000000</cpu>
        <n-cpus>1</n-cpus>
        <ram>134217728</ram>
        <storage>1073741824</storage>
      </minimum>
      <recommended>
        <cpu>1000000000</cpu>
        <n-cpus>1</n-cpus>
        <ram>805306368</ram>
        <storage>4294967296</storage>
      </recommended>
    </resources>

    <variant id="standard">
      <name>Alpinelinux 3.18 Standard</name>
    </variant>
    <variant id="extended">
      <name>Alpinelinux 3.18 Extended</name>
    </variant>
    <variant id="virtual">
      <name>Alpinelinux 3.18 Virtual</name>
    </variant>
    <variant id="xen">
      <name>Alpinelinux 3.18 Xen</name>
    </variant>

    <!-- Standard Installer !-->
    <media arch="aarch64">
      <variant id="standard"/>
      <iso>
        <volume-id>alpine-std 3\.18.* aarch64$</volume-id>
      </iso>
      <kernel>boot/vmlinuz-lts</kernel>
      <initrd>boot/initramfs-lts</initrd>
    </media>
    <media arch="i686">
      <variant id="standard"/>
      <iso>
        <volume-id>alpine-std 3\.18.* x86$</volume-id>
